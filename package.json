{
  "name": "@lernato/common-angular",
  "version": "6.0.0",
  "license": "MIT",
  "description": "JavaScript utilities for writing code the @lernato way.",
  "author": "Lernato, LLC",
  "repository": {
    "type": "git",
    "url": "https://github.com/LernatoLLC/common-angular.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc && npm run fix-ts-paths",
    "fix-ts-paths": "node scripts/fix-ts-paths.js",
    "doc": "typedoc --out ../docs/utils src/**/*.ts",
    "lint": "tslint --project . src/**/*.ts --force --format stylish",
    "prepush": "npm run build",
    "push": "npm publish --access public",
    "push:major": "npm version major --no-git-tag-version && npm run push",
    "push:minor": "npm version minor --no-git-tag-version && npm run push",
    "push:patch": "npm version patch --no-git-tag-version && npm run push",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "peerDependencies": {
    "@lernato/common": "^4.x",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/common": "9.0.0",
    "@angular/compiler": "9.0.0",
    "@angular/core": "9.0.0",
    "@angular/platform-browser": "9.0.0",
    "@angular/router": "9.0.0",
    "@lernato/common": "4.1.1",
    "@types/jest": "25.1.2",
    "jest": "25.1.0",
    "rimraf": "3.0.1",
    "rxjs": "6.5.4",
    "ts-jest": "25.2.0",
    "tslint": "5.18.0",
    "typedoc": "0.16.9",
    "typescript": "3.7.5",
    "zone.js": "~0.10.2",
    "codelyzer": "5.2.1",
    "prettier": "1.19.1",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.1.0"
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {}
}
